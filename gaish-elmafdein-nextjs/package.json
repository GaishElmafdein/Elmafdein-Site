{
  "name": "gaish-elmafdein-nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000 --turbopack",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:all": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "format": "prettier -w .",
    "prepare": "husky",
    "test:proxy": "node ../scripts/test-proxy.mjs",
    "test:proxy:vercel": "cross-env TEST_BASE=$VERCEL_URL node ../scripts/test-proxy.mjs",
    "test:proxy:local": "cross-env TEST_BASE=http://localhost:3000 node ../scripts/test-proxy.mjs",
    "test:proxy:ps": "pwsh -NoProfile -File ../scripts/test-proxy.ps1 -Base http://localhost:3000"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.54.0",
    "@types/pdf-parse": "^1.1.5",
    "cohere-ai": "^7.18.0",
    "framer-motion": "^12.23.12",
    "hls.js": "^1.6.9",
    "lucide-react": "^0.537.0",
    "mammoth": "^1.10.0",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "openai": "^5.12.2",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-pdf": "^9.1.1",
    "react-player": "^3.3.1"
  },
  "lint-staged": {
    ".{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    ".{css,md,json}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.10",
    "typescript": "^5"
  }
}
