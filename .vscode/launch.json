{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: dev",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}/gaish-elmafdein-nextjs",
      "env": { "PORT": "3000" }
    },
    {
      "name": "Next.js: build & start",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run build && npm run start",
      "cwd": "${workspaceFolder}/gaish-elmafdein-nextjs",
      "env": { "PORT": "3000" }
    }
  ]
}
